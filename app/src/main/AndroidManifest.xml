<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rabor.servicedemo" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.rabor.servicedemo.AppleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rabor.servicedemo.BaconActivity"
            android:label="@string/title_activity_bacon" >
        </activity>

        <service android:name="com.rabor.servicedemo.MyIntentService" >
        </service>
        <service
            android:name="com.rabor.servicedemo.MyService" >
        </service>
    </application>

</manifest>
